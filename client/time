module('time', {}, function(require) {

	var time = {

		get: function() { return new Date().getTime() },

		delta: function() {
			var time = this
			var d = {
				last: time.get(),
				get: function() {
					var now = time.get()
					var result = now - this.last
					this.last = now
					return result } }

			return d } }

	return time })
